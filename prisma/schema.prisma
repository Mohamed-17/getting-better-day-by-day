

generator client {
  provider = "prisma-client-js"
}

datasource db {
 provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Project {
  id    String @id @default(cuid()) @map("_id")
  name  String
  image String
  description String
  category String
  link String
  createDate String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model Book {
id String @id @default(cuid()) @map("_id")
title String
author String
image String
status String
createdAt DateTime @default(now())
updatedAt DateTime @updatedAt
}